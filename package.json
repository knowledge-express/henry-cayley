{
  "name": "henry-cayley",
  "version": "1.0.0",
  "description": "A cayley library written in TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "pretest": "npm run clean && tsc --inlineSourceMap --outDir lib",
    "test": "nyc ava",
    "posttest": "npm run clean && npm run build",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublish": "npm run build"
  },
  "authors": [
    "Joost Verdoorn <jpverdoorn@gmail.com>",
    "Steffan Sluis <steffansluis@gmail.com>"
  ],
  "repository": "git+https://github.com/knowledge-express/henry-cayley.git",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/knowledge-express/henry-cayley/issues"
  },
  "homepage": "https://github.com/knowledge-express/henry-cayley#readme",
  "dependencies": {
    "@types/node": "^8.0.10",
    "@types/node-fetch": "^1.6.7",
    "@types/query-string": "^4.3.1",
    "nock": "^9.0.14",
    "node-fetch": "^1.7.1",
    "query-string": "^4.3.4",
    "readable-stream": "^2.3.3",
    "response-to-rx": "^1.0.2",
    "text-encoding": "^0.6.4",
    "typescript": "^2.4.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "codecov": "^1.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
